                    "type": "ifUser",
                    "constraint": "subscribed",
                    "value": 312  // campaign uid
                },
                {
                    "type": "hasTag",
                    "constraint": "hasNot",
                    "value": 123 // tag uid
                }
            ],
        }
        
+ Response 200 (application/json)

        {
            "success"                   :   true
        }        


